 hooks violation\n    const nextAction = getNextAction();\n\n    // DEBUG: Log render state\n    console.log('üé® RENDER CHECK - loading:', loading, '| currentJob:', currentJob ? 'EXISTS (id: ' + currentJob.data?.id + ')' : 'NULL');\n\n    // Render loading screen (auth and role already checked in wrapper)\n    if (loading) {\n      console.log('‚û°Ô∏è Rendering LOADING screen');\n      return /*#__PURE__*/(0, _reactJsxDevRuntime.jsxDEV)(_reactNativeSafeAreaContext.SafeAreaView, {\n        style: styles.container,\n        children: /*#__PURE__*/(0, _reactJsxDevRuntime.jsxDEV)(View.default, {\n          style: styles.loadingContainer,\n          children: /*#__PURE__*/(0, _reactJsxDevRuntime.jsxDEV)(ActivityIndicator.default, {\n            size: \"large\",\n            color: \"#210059\"\n          }, void 0, false, {\n            fileName: _jsxFileName,\n            lineNumber: 1861,\n            columnNumber: 11\n          }, this)\n        }, void 0, false, {\n          fileName: _jsxFileName,\n          lineNumber: 1860,\n          columnNumber: 9\n        }, this)\n      }, void 0, false, {\n        fileName: _jsxFileName,\n        lineNumber: 1859,\n        columnNumber: 7\n      }, this);\n    }\n\n    // Render idle screen (clean map view OR job pending acceptance)\n    const renderIdleScreen = () => {\n      console.log('‚û°Ô∏è Rendering IDLE screen', currentJob ? '(job pending acceptance)' : '(no job)');\n      console.log('üó∫Ô∏è Initializing idle map...');\n      return /*#__PURE__*/(0, _reactJsxDevRuntime.jsxDEV)(Animated.default.View, {\n        style: [styles.container, {\n          opacity: fadeAnim\n        }],\n        children: /*#__PURE__*/(0, _reactJsxDevRuntime.jsxDEV)(_reactNativeSafeAreaContext.SafeAreaView, {\n          style: styles.container,\n          children: /*#__PURE__*/(0, _reactJsxDevRuntime.jsxDEV)(View.default, {\n            style: styles.container,\n            children: [Platform.default.OS === 'web' ? /*#__PURE__*/(0, _rea